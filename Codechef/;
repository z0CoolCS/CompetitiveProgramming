#include<bits/stdc++.h>

using namespace std;

#define REP(i,a,b) for(int i=a;i<b;i++)

bool matrix[1001][1001];
int n,m;

bool isvalid(int x,int y){
  return 0<=x && x<n && y>=0 && y<n;
}
void setmatrix(int x,int y){
  REP(i,0,n){
    matrix[i][y]=1;
    matrix[x][i]=1;
  }
  int mini=min(x,y);
  REP(i,0,16){
    
  }
}
int main(){

  int t;
  int x,y;
  cin>>t;
  while(t--){
    cin>>n>>m;
    memset(matrix,0,sizeof matrix);
    REP(i,0,m){
      cin>>x>>y;
      setmatrix(x,y);
    }
    
  }

  return 0;
}
